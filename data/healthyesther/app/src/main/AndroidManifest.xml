<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.robwilliamson.healthyesther" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name=".App">
        <activity
            android:name="com.robwilliamson.healthyesther.EventActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.robwilliamson.healthyesther.HomeActivity"
            android:label="@string/title_activity_home" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.robwilliamson.healthyesther.add.Meal"
            android:label="@string/log_a_meal">
        </activity>
        <activity android:name="com.robwilliamson.healthyesther.add.Medication"
            android:label="@string/log_medication">
        </activity>
    </application>

</manifest>
